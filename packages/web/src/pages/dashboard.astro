---
import DashboardLayout from "@/layout/dashboard-layout.astro";

const { userId, redirectToSignIn } = Astro.locals.auth();

if (!userId) {
	return redirectToSignIn({
		returnBackUrl: Astro.request.url,
	});
}
---
<DashboardLayout
	title="Dashboard"
	buttons={[{
		label: "Docs",
		href: "https://docs.giolt.com"
	}]}
>
</DashboardLayout>
